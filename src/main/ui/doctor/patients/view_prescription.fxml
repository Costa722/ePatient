                                                <Label styleClass="text-box-header" text="DISEASE NAME" />
                                                <TextField fx:id="diseaseNameTv" editable="false" prefHeight="49.0" prefWidth="65.0" styleClass="text-field-normal" />
                                             </children>
                                             <padding>
                                                <Insets right="10.0" />
                                             </padding>
                                             <VBox.margin>
                                                <Insets top="10.0" />
                                             </VBox.margin>
                                          </VBox>
