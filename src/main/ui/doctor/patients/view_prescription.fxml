                                    </VBox>
                                    <HBox prefWidth="200.0">
                                       <children>
                                          <Label styleClass="text-box-header" text="OBSERVED DISEASES" />
                                       </children>
                                    </HBox>
                                    <ListView fx:id="observedDiseases" prefHeight="200.0" prefWidth="200.0" stylesheets="@../../../../resources/listview_styles_ash.css" VBox.vgrow="ALWAYS" />
                                 </children>
                                 <padding>
                                    <Insets bottom="20.0" left="50.0" right="50.0" top="20.0" />
                                 </padding>
                                 <VBox.margin>
                                    <Insets />
                                 </VBox.margin>
                              </VBox>
                           </children>
                        </VBox>
                     </content>
                  </Tab>
                  <Tab text="SUGGEST MEDICAL TESTS">
